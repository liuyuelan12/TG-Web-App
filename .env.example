# Database Configuration
# For local development, use SQLite:
# DATABASE_URL="file:./dev.db"
# For production on Render, use PostgreSQL:
DATABASE_URL="postgresql://user:password@host:port/database?schema=public"

# NextAuth Configuration
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-key-min-32-characters-long"

# JWT Configuration
JWT_SECRET="your-jwt-secret-key"

# Node Environment
NODE_ENV="development"

# Telegram API (Required for Telegram operations)
API_ID="22453265"
API_HASH="641c3fad1c94728381a70113c70cd52d"

# Session Configuration (Optional)
SESSION_TIMEOUT="7200000"

# Admin Configuration (Optional)
ADMIN_EMAIL=""

# Proxy Configuration (Optional - 支持动态更新，推荐在 Railway 使用环境变量配置)
# 格式: socks5://username:password@addr:port,socks5://username:password@addr:port
# 多个代理用英文逗号分隔
PROXY_CONFIGS="socks5://username:password@102.177.147.43:50101,socks5://username:password@102.177.146.2:50101"

# Data Storage Paths (Optional - Railway 上推荐使用持久化卷)
SESSIONS_DIR="/app/sessions"
MEDIA_DIR="/app/scraped_data"
PROJECT_ROOT="/app"
